{% extends "StefBVBundle::layout.html.twig" %}

{% block body %}

    <div id="main-wrapper">
        <div id="main" class="container">
            <h1>{{ page.title }}</h1>
            {{ page.body|raw }}

            <form action="{{ path('stef_bvbundle_contact') }}" method="post" {{form_enctype(form) }}>
                {{ form_widget(form) }}


                <button type="submit">Send</button>


            </form>
        </div>
    </div>

{% endblock %}